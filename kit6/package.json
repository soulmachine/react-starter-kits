    "file-loader": "^0.8.5",
    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "open-browser-webpack-plugin": "0.0.2",
    "precommit-hook": "^3.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"

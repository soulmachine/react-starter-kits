{
  "name": "kit6",
  "version": "1.0.0",
  "description": "A React starter kit",
  "main": "src/main.jsx",
  "scripts": {
    "build": "webpack -d",
    "start": "webpack-dev-server --devtool source-map --hot --inline --progress --colors --history-api-fallback --content-base build",
